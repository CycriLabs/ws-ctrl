    "@eslint/js": "^9.33.0",
    "@types/prompts": "^2.4.9",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.33.0",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "memfs": "^4.36.0",
    "prettier": "^3.6.2",
    "tempy": "^3.1.0",
    "tsup": "^8.5.0",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.1",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "chalk": "^5.5.0",
    "commander": "^14.0.0",
    "conf": "^14.0.0",
    "dotenv": "^17.2.1",
    "prompts": "^2.4.2"
  }
